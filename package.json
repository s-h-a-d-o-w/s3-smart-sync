{
  "name": "s3-smart-sync",
  "version": "1.0.0",
  "main": "./src/server.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build:client": "rimraf dist && pnpm build:bundle && pnpm build:exe && timeout 5 && pnpm silenceexe && pnpm changeicon && pnpm build:zip",
    "build:bundle": "rimraf dist && esbuild ./src/client/index.ts --bundle --platform=node --target=node20 --outfile=dist/index.cjs",
    "build:exe": "build-exe.bat",
    "build:zip": "build-zip.bat",
    "changeicon": "exe-edit s3-smart-sync.exe s3-smart-sync.exe --icon ./assets/icon.ico",
    "silenceexe": "create-nodew-exe s3-smart-sync.exe s3-smart-sync.exe",
    "start": "tsx .",
    "start:client": "tsx ./src/client",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@types/auto-launch": "^5.0.5",
    "@types/aws-lambda": "^8.10.145",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.17.10",
    "@types/node": "^20.15.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.14.1",
    "auto-launch": "^5.0.6",
    "chokidar": "^4.0.1",
    "create-nodew-exe": "^1.0.10",
    "esbuild": "^0.24.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "exe-edit": "^0.1.1",
    "lodash": "^4.17.21",
    "node-tray": "^1.0.1",
    "prettier": "3.3.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2",
    "winston": "^3.15.0",
    "ws": "^8.18.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.665.0",
    "@aws-sdk/client-sns": "^3.665.0",
    "body-parser": "^1.20.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "tsx": "^4.19.1"
  }
}
